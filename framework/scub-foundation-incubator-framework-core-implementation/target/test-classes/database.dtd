<!ELEMENT dataset (
   (NUMBER_VALUE|
    PERSONNE_TYPE)*)>

<!ELEMENT NUMBER_VALUE EMPTY>
<!ATTLIST NUMBER_VALUE
    DOUBLE_VALUE CDATA #IMPLIED
    FLOAT_VALUE CDATA #IMPLIED
    ID CDATA #REQUIRED
    INT_VALUE CDATA #IMPLIED
    LONG_VALUE CDATA #IMPLIED 
    SHORT_VALUE CDATA #IMPLIED
    VERSION CDATA #REQUIRED
>

<!ELEMENT PERSONNE_TYPE EMPTY>
<!ATTLIST PERSONNE_TYPE
    ID CDATA #REQUIRED
    LABEL CDATA #REQUIRED
    VERSION CDATA #REQUIRED
>

